# OCR Middleware Configuration
# Copy this file to .env and customize for your environment

# Application metadata
APP_NAME=Symbiota OCR Middleware
DISPLAY_NAME=OCR Service
MODEL_NAME=Default Model

# Version information
SERVER_VERSION=1.0.0
API_VERSION=1.0.0

# Server configuration
# HOST: 0.0.0.0 to listen on all interfaces (inside container)
# PORT: Default 8000, will be mapped by docker-compose
HOST=0.0.0.0
PORT=8000

# Model configuration
# Path to models directory (mounted via docker-compose)
# Models should be organized in subdirectories under this path
MODEL_PATH=/app/models

# Azure OCR Service (optional)
# Set to your Azure OCR endpoint URL for production use
AZURE_ROUTE=
